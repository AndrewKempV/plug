{
  "name": "plugg",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package && node ./android/fix/android-release-fix.js && npx jetify",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:packager": "env REACT_DEBUGGER=\"$([ -d '/Applications/React Native Debugger.app' ] && echo \"open 'rndebugger://set-debugger-loc?host=localhost&port=8081' --args\")\" node node_modules/react-native/local-cli/cli.js start",
    "start:reset": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "build:android:debug": "cd android && ./gradlew clean && cd ../ && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew clean && cd ../ && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleRelease",
    "build:android:clean": "cd android && ./gradlew clean",
    "build:ios:release": "set -o pipefail && xcodebuild -workspace ios/plugg.xcworkspace -scheme plugg -configuration Release COMPILER_INDEX_STORE_ENABLE=NO archive -archivePath /Users/eweibman/github/plugg-fix/plugg-mobile/plugg.xcarchive | xcpretty && cp -R /Users/eweibman/github/plugg-fix/plugg-mobile/plugg.xcarchive /Users/eweibman/Library/Developer/Xcode/Archives",
    "build:ios:release:archive": "set -o pipefail && xcodebuild -workspace ios/plugg.xcworkspace -scheme plugg -configuration Release COMPILER_INDEX_STORE_ENABLE=NO archive -archivePath /var/folders/6q/wgy6jtp12w5gzgm9lzcglpqw0000gn/T/__archive__360283912/plugg.xcarchive | xcpretty",
    "build:ios:release:export": "set -o pipefail && xcodebuild -exportArchive -archivePath /var/folders/6q/wgy6jtp12w5gzgm9lzcglpqw0000gn/T/__archive__360283912/plugg.xcarchive -exportPath /var/folders/6q/wgy6jtp12w5gzgm9lzcglpqw0000gn/T/__export__272954055 -exportOptionsPlist /Users/vagrant/deploy/export_options.plist | xcpretty",
    "test": "jest -u",
    "watch": "tsc -w",
    "lint": "eslint",
    "lint:all": "eslint './app/**/*.{ts,tsx}'",
    "lint:fix": "eslint './app/**/*.{ts,tsx}' --fix",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "android:emulator": "cd $ANDROID_HOME/tools && emulator -list-avds && emulator -avd Pixel_2_XL_API_28",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator=\"iPhone 8\"",
    "ios:install": "yarn install && cd ios && pod install",
    "ios:device:development": "node node_modules/react-native/local-cli/cli.js run-ios --udid 4a8d84fa34722233f1b6bdff87fcf70c3add7d14",
    "ios:device:release": "node node_modules/react-native/local-cli/cli.js run-ios --udid 4a8d84fa34722233f1b6bdff87fcf70c3add7d14 --configuration Release",
    "ios:reset": "rm -rf ios/build/ModuleCache",
    "ios:surrender": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules && yarn cache clean --force && yarn ios:install",
    "clean-install": "rm -rf node_modules && npm cache clean --force && watchman watch-del-all && yarn ios:install",
    "clean-bundler": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all",
    "link-assets": "react-native unlink react-native-vector-icons && react-native link react-native-vector-icons",
    "test:e2e": "detox test -c ios.sim.debug"
  },
