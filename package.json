    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/plugg.app",
        "build": "xcodebuild -workspace ios/plugg.xcworkspace -scheme plugg -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
